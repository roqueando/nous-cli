{
  "name": "nous-cli",
  "description": "Une interface en ligne de commande pour nous",
  "version": "0.4.7",
  "author": "Vitor Roque",
  "bin": {
    "nous": "./bin/run"
  },
  "bugs": "https://github.com/roqueando/nous-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@roqueando/nous": "^0.7.0",
    "@types/inquirer": "^6.5.0",
    "boxen": "^4.2.0",
    "cfonts": "^2.8.1",
    "chalk": "^3.0.0",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.4.5",
    "execa": "^4.0.0",
    "figlet": "^1.3.0",
    "inquirer": "^7.1.0",
    "ncp": "^2.0.0",
    "ora": "^4.0.3",
    "pkg-install": "^1.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/execa": "^2.0.0",
    "@types/figlet": "^1.2.0",
    "@types/gradient-string": "^1.1.0",
    "@types/mocha": "^5",
    "@types/ncp": "^2.0.3",
    "@types/node": "^12.12.31",
    "@types/ora": "^3.2.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/roqueando/nous-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nous",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "monter": {
        "description": "Up all services and manager"
      },
      "monter:service": {
        "description": "Up a independent service"
      }
    }
  },
  "repository": "roqueando/nous-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && cp ./src/squelette/package.json ./lib/squelette/package.json && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
